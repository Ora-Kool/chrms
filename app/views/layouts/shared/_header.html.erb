<header id="header" class="header fixed"  itemscope="" itemtype="http://schema.org/WPHeader">
	<div class="top-bar">
  		<div class="top-bar-title" style="margin-top: 5px;">
    		<span data-responsive-toggle="responsive-menu" data-hide-for="medium">
     			<button class="menu-icon white" type="button" data-toggle></button>
          MENU
    		</span>
    		<!--<strong>MENU</strong>-->
  </div>
  <div id="responsive-menu">
    <div class="top-bar-left">
      <ul class="menu">
        <li><%= link_to 'CHRMS', root_path %></a></li>
      </ul>
    </div>
    <div class="top-bar-right">
      <ul class="menu">
        <% if logged_manager? %>
          <li><%=  link_to 'Dashboard', manager_dashboard_path %></li>
          <li><%=  link_to 'Logout', managers_session_logout_path, method: :delete%></li>
        <%elsif logged_doctor?%>
          <li><%=  link_to 'Dashboard', doctor_dashboard_path %></li>
          <li><%=  link_to 'Logout', doctor_logout_path, method: :delete %></li>
        <%else%>
          <li><%=  link_to 'Login', login_panel_path, class: 'fi-arrow-right'%></li>
        <%end%>
        
        
      </ul>
    </div>
     <div class="menu-centered">
      <ul class="menu main">
        <!--<li><%= link_to 'Home', root_path %></li>-->
        <li><%= link_to 'About', about_path %></li>
        <li><%= link_to 'Contact Us', contact_path%></li>
      </ul>
    </div>
  </div>
  <%= render 'layouts/shared/alert' %>
</div>
</header>

